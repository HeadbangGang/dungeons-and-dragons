  "scripts": {
    "build": "rimraf dist && webpack",
    "clean": "rimraf node_modules",
    "lint:css": "stylelint '**/*.scss' --color --fix",
    "lint:js": "eslint src --ext .jsx,.js --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "npm run start:firebase & NODE_ENV=development webpack-dev-server",
    "startquiet": "npm run start -- --no-open & npm run start:firebase",
    "start:firebase": "firebase emulators:start --export-on-exit=./firebase-mock-data --import=./firebase-mock-data"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
